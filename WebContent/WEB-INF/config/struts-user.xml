<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="user-struts" extends="my-struts" namespace="/user">
		<action name="*" class="com.jh.action.UserAction" method="{1}">
			<result name="id" type="json"></result>
			<result name="pager" type="json"></result>
			<result name="all" type="json"></result>
			<result name="save" type="json"></result>
			<result name="update" type="json"></result>
			<result name="del" type="json"></result>
			<result name="login_page">/WEB-INF/views/login.jsp</result>
			<result name="login">/WEB-INF/views/home1.jsp</result>
		</action>
	</package>
</struts>